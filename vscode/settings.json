      "commands": ["workbench.action.terminal.focus"]
    },
    {
      "before": ["<leader>", "w", "f"],
      "commands": ["workbench.action.toggleFullScreen"]
    },
    {
      "before": ["<leader>", "w", "s"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<leader>", "w", "k"],
      "commands": ["workbench.action.openGlobalKeybindings"]
    },
    {
      "before": ["<leader>", "d", "d"],
      "after": [],
      "commands": ["workbench.action.debug.start"]
    },
    {
      "before": ["<leader>", "d", "s"],
      "after": [],
      "commands": ["workbench.action.debug.stop"]
    },
    {
      "before": ["<leader>", "e", "h"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "code C:/Windows/System32/Drivers/etc/hosts\u000D"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "g", "p"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.terminal.sendSequence",
          "args": {
            "text": "gfp\u000D"
          }
        }
      ]
    },
    //GIT/GITLENS
    {
      "before": ["<leader>", "g", "l"],
      "after": [],
      "commands": ["gitlens.views.lineHistory.focus"]
    },
    {
      "before": ["<leader>", "g", "f"],
      "after": [],
      "commands": ["gitlens.views.fileHistory.focus"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "after": [],
      "commands": ["gitlens.diffWithPrevious"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "after": [],
      "commands": ["gitlens.showRepositoriesView"]
    },
    {
      "before": ["<leader>", "g", "h"],
      "after": [],
      "commands": ["gitlens.showQuickFileHistory"]
    },
    {
      "before": ["<leader>", "g", "g"],
      "after": [],
      "commands": ["gitlens.openWorkingFileInDiffRight"]
    },
    {
      "before": ["<leader>", "g", "y"],
      "after": [],
      "commands": ["gitlens.views.searchAndCompare.swapComparison"]
    },
    {
      "before": ["<leader>", "g", "c"],
      "after": [],
      "commands": ["gitlens.views.searchAndCompare.refresh"]
    },
    {
      "before": ["<leader>", "g", "o"],
      "after": [],
      "commands": ["gitlens.openChangedFiles"]
    },
    {
      "before": ["<leader>", "g", "t"],
      "after": [],
      "commands": ["gitlens.toggleLineBlame"]
    },
    //SNIPPETS
    {
      "before": ["<leader>", "s"],
      "after": ["i"],
      "commands": [
        {
          "command": "editor.action.showSnippets"
        }
      ]
    },
    //BOOKMARKKS
    //normal vim stuff
    {
      "before": ["*"],
      "after": ["*", "z", "z"]
    },
    {
      "before": ["#"],
      "after": ["#", "z", "z"]
    }
