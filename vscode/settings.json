  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["C-space"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<leader>", "k"],
      "commands": ["workbench.action.navigateUp"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["workbench.action.navigateDown"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<C-w>"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["search.action.focusNextSearchResult"]
    },
    {
      "before": ["<leader>", "N"],
      "commands": ["search.action.focusPreviousSearchResult"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    // GOTOS
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["references-view.find"]
    },
    {
      "before": ["g", "n"],

      "commands": ["editor.action.dirtydiff.next"]
    },
    //PROBLEMS
    {
      "before": ["<leader>", "p"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "P"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    // {
    //   "before": ["<leader>", "p"],
    //   "commands": ["workbench.action.pinEditor"]
    // },
    // {
    //   "before": ["<leader>", "P"],
    //   "commands": ["workbench.action.unpinEditor"]
    // },
    //WINDOW
    {
      "before": ["<leader>", "w", "b"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "w", "e"],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closePanel"
      ]
    },
    {
      "before": ["<leader>", "w", "o"],
      "commands": ["workbench.action.output.toggleOutput"]
    },
    {
      "before": ["<leader>", "w", "x"],
      "commands": ["workbench.view.extensions"] //extensions
    },
    {
      "before": ["<leader>", "w", "h"], //move editor left
      "commands": ["workbench.action.moveActiveEditorGroupLeft"]
    },
    {
      "before": ["<leader>", "w", "l"], //move editor right
      "commands": ["workbench.action.moveActiveEditorGroupRight"]
    },
    {
      "before": ["<leader>", "w", "u"],
      "commands": ["outline.focus"]
    },
    {
      "before": ["<leader>", "w", "p"],
      "commands": ["workbench.actions.view.problems"]
    },
    {
      "before": ["<leader>", "w", "g"],
      "commands": ["workbench.scm.focus"]
