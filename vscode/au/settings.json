  "editor.formatOnSave": true,
  "editor.autoIndent": "advanced",
  "vim.useSystemClipboard": true,
  "vim.sneak": true,
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.smartRelativeLine": true,
  "whichkey.delay": 0,
